{
  "name": "en-node-graph-gui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "npm run deploy-web;",
    "deploy-both": "npm run deploy-web; npm run deploy-lib;",
    "deploy-web": "npm run build; npm run netlify; npm run github;",
    "github": "git add .; git commit -am \"front-end-deploy\"; git push -u origin master;",
    "netlify-lib": "netlify deploy -s REPLACE_ME_NETLIFY_LIB_UUID --dir=dist --prod",
    "deploy-lib": "vue-cli-service build --target lib --name effectnode ./src/oso-public/oso-viewer.vue; npm run netlify-lib",
    "netlify": "netlify deploy --dir=\"./dist\" --prod",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.3.0",
    "core-js": "^2.6.5",
    "dagre": "^0.8.4",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  }
}
